{
  "name": "quadmins-test",
  "version": "1.0.0",
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  },
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "10.13.0",
    "npm": "5.0.3"
  },
  "scripts": {
    "lint": "standard --fix",
    "test": "cross-env NODE_ENV=test mocha  __tests__/server/**/*.test.js",
    "report": "cross-env NODE_ENV=test nyc --reporter=lcov mocha --timeout 5000 __tests__/server/**/*.test.js",
    "nyc": "cross-env NODE_ENV=test nyc mocha --timeout 5000 __tests__/server/**/*.test.js",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "JoniAguero",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-formidable": "^1.2.0",
    "jsonwebtoken": "^8.5.0",
    "moment": "^2.24.0",
    "mongoose": "^5.4.19",
    "morgan": "^1.9.1",
    "multer": "^1.4.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "chalk": "^2.4.2",
    "faker": "^4.1.0",
    "mocha": "^6.0.2",
    "nock": "^10.0.6",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "standard": "^12.0.1"
  },
  "nyc": {
    "exclude": [
      "app.js",
      "_helpers/**/*.js",
      "__tests__",
      "node_modules"
    ],
    "cache": false,
    "report-dir": "./__tests__/report"
  }
}
